{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "max-classes-per-file": "off",
    "object-curly-newline": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off"
  }
}
